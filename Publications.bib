	keywords = {Adaptation, Physiological, Gene Expression Profiling, Gene Ontology, Genome, Protozoan, MicroRNAs, Paramecium, RNA Interference, RNA, Messenger, RNA, Small Interfering, Transcriptome},
	pages = {8036--8049},
	file = {Volltext:/Users/mschulz/Zotero/storage/NHST4WSW/Karunanithi et al. - 2019 - Exogenous RNAi mechanisms contribute to transcript.pdf:application/pdf},
