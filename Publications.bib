	pmcid = {PMC6612827},
	keywords = {Gene Regulatory Networks, Humans, Neoplasms, RNA},
	pages = {i596--i604},
	file = {Volltext:/Users/mschulz/Zotero/storage/F7LNTE99/List et al. - 2019 - Large-scale inference of competing endogenous RNA .pdf:application/pdf},
