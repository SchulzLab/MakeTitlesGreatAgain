	keywords = {Data Curation, Electronic Data Processing, Humans, Word Processing},
	pages = {bav050},
	file = {Volltext:/Users/mschulz/Zotero/storage/PZEQH6ZU/Ebert et al. - 2015 - A general concept for consistent documentation of .pdf:application/pdf},
}

@article{schulz_fiona_2014,
